{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.1.0",
        "arielmejiadev/larapex-charts": "^6.0",
        "barryvdh/laravel-dompdf": "^2.0.1",
        "guzzlehttp/guzzle": "^7.8",
        "jeroennoten/laravel-adminlte": "^3.14.3",
        "laravel/framework": "^10.20",
        "laravel/sanctum": "^3.2.6",
        "laravel/tinker": "^2.8.1",
        "laravel/ui": "^4.2.2",
        "livewire/livewire": "^2.12.6",
        "luecano/numero-a-letras": "^3.0",
        "maatwebsite/excel": "^3.1.48",
        "spatie/laravel-permission": "^5.10.2",
        "yajra/laravel-datatables-oracle": "^10.8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel-lang/common": "^3.1.1",
        "laravel/pint": "^1.11",
        "laravel/sail": "^1.23.4",
        "mockery/mockery": "^1.6.6",
        "nunomaduro/collision": "^7.8.1",
        "phpunit/phpunit": "^10.3.2",
        "spatie/laravel-ignition": "^2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
